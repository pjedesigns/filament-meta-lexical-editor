{
    "name": "pjedesigns/filament-meta-lexical-editor",
    "description": "Implementation of meta's lexical editor in FilamentPHP, a modern, extensible text editor framework.",
    "keywords": [
        "pjedesigns",
        "laravel",
        "filamentPHP",
        "editor",
        "text-editor",
        "rich-text",
        "filament-meta-lexical-editor"
    ],
    "homepage": "https://github.com/pjedesigns/filament-meta-lexical-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Egan",
            "email": "paul@pjedesigns.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Pjedesigns\\FilamentMetaLexicalEditor\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Pjedesigns\\FilamentMetaLexicalEditor\\FilamentMetaLexicalEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
